package CroudWolf;

import static org.junit.Assert.*;

import org.junit.Before;
import org.junit.Test;

import Worker.Worker;
import Pago.Pago;
import PaqueteDeTareas.PaqueteDeTareas;
import Proyecto.Proyecto;

import static org.mockito.Mockito.*;

import java.util.List;
public class TestCroudWolf {

	
	CroudWolf croudWolf1 = new CroudWolf();
	croudWolf1.agregarWorker(worker1);
	croudWolf1.agregarProyecto(proyecto1);
	croudWolf1.notificarPaqueteCompleto(paquete1);
	
	
	
	@Before
	public void setUp() throws Exception {
	
 Proyecto 	proyecto1 = mock(Proyecto.class);
 Pago 	pago1 = mock(Pago.class);
 Worker worker1 = mock (Worker.class);
 PaqueteDeTareas paquete1 = mock (PaqueteDeTareas.class);
 
 
 	

	}

	@Test
	public void testCreador() {
		assertNotNull(croudWolf1.getWorkers());
		assertNotNull(croudWolf1.getProyectos());
	}
	
	@Test
	public void test() {
		verify(proyecto1).notificarWorkers(proyecto1);
		verify(paquete1).getProyecto();
	}

}
